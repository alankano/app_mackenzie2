<Calculadora>:
    BoxLayout:
        orientation: "vertical"
        MDToolbar:
            title : 'Calculadora de equa\u00E7\u00E3o do segundo grau'
            left_action_items : [["arrow-left", lambda x : root.voltar()]]
    
            MDIconButton:
                icon: "book"
                on_release : app.root.current = 'introducao1'
            MDIconButton:
                icon: "pencil-outline"
                on_release : app.root.current = 'exercicios'   

        MDLabel:
            text: "Calculadora de equa\u00E7\u00E3o do segundo grau : 0 = ax\u00B2 + bx + c"
            halign: "center"

        BoxLayout:
            orientation:'vertical'
            spacing : '30dp'
            MDLabel : 
                pos_hint : {"center_x": 0.5, "center_y": 0.5}
                id : calculadora_label
                text : root.resposta
                halign: 'center'
            MDTextField :
                hint_text : 'a'
                size_hint : 0.15, None
                pos_hint : {"center_x": 0.5, "center_y": 0.5}
                required : True
                id :variavel_a
                max_text_length : 4
                normal_color : 1,1,1,1
                input_filter : 'int'
            MDTextField :
                hint_text : 'b'
                size_hint : 0.15, None
                pos_hint : {"center_x": 0.5, "center_y": 0.5}
                required : True
                id : variavel_b    
                max_text_length : 4
                normal_color : 1,1,1,1
                input_filter : 'int'
            MDTextField :
                hint_text : 'c'
                size_hint : 0.15, None
                pos_hint : {"center_x": 0.5, "center_y": 0.5}
                required : True
                id : variavel_c
                max_text_length : 4
                normal_color : 1,1,1,1
                input_filter : 'int'
            MDRoundFlatButton : 
                text :'Calcular'
                on_release: root.calcular()
                pos_hint : {"center_x": 0.5, "center_y": 0.5}
                theme_text_color : "Primary"
                text_color: 0,0,1,1

<Exercicios>:
    on_pre_enter: root.criando_exercicios()
    BoxLayout:
        orientation: "vertical"
        MDToolbar:
            left_action_items : [["arrow-left", lambda x : root.voltar()]] 

        BoxLayout:
            orientation: 'vertical'
            MDLabel: 
                padding_x : 20
                halign: 'center'
                text: 'Nesta se\u00E7\u00E3o voc\u00EA far\u00E1 5 exerc\u00EDcios seguidos sem poder acessar o material e nem a calculadora. Est\u00E1 pronto para come\u00E7ar ?'
            FloatLayout:
                orientation: 'horizontal'

                MDRoundFlatButton:
                    text: "Come\u00E7ar exerc\u00EDcios."
                    pos : (150,50)
                    on_release : app.root.current ='prova1'

                MDRoundFlatButton:
                    text: "Estudar mais um pouco."
                    pos : (450,50)
                    on_release : app.root.current = 'introducao1'


<Prova1>:        
    on_pre_enter : root.comecando_exercicios()
    BoxLayout:
        orientation: "vertical"                
        MDToolbar:
            title :"Desistir"
            left_action_items : [["arrow-left", lambda x : root.voltar()]]
        FloatLayout:
            spacing : '20dp'
            MDLabel :
                pos_hint : {'center_x' : 0.5, 'center_y' : 0.9 }
                halign : 'center'
                height : self.texture_size[1]
                id: prova1_enunciado
            MDRaisedButton : 
                id : prova1_alternativa1
                on_release: root.salvando_os_dados_do_usuario('a')
                pos : (350, 350)
            MDRaisedButton : 
                id: prova1_alternativa2
                on_release: root.salvando_os_dados_do_usuario('b')
                pos : (350, 300)
            MDRaisedButton : 
                id: prova1_alternativa3
                on_release: root.salvando_os_dados_do_usuario('c')
                pos : (350, 250)
            MDRaisedButton : 
                id: prova1_alternativa4
                on_release: root.salvando_os_dados_do_usuario('d')
                pos : (350, 200)
            MDRaisedButton : 
                id: prova1_alternativa5
                on_release: root.salvando_os_dados_do_usuario('e')
                pos : (350, 150)

