<Menu>:
    BoxLayout:
        orientation:'vertical'
        padding : 100
        spacing : 50
        canvas :
            Color : 
                rgba : 1, 1, 1, 1
            Rectangle :
                size: self.size
                pos : self.pos
        Image : 
            source : 'logo_mackenzie.png'
            pos_hint : {"center_x": 0.5, "center_y": 0.5}
            height: 100
        MDRoundFlatButton :
            text: 'Introdução a equação de segundo grau.'
            on_release: app.root.current = 'introducao1'
            pos_hint : {"center_x": 0.5, "center_y": 0.5}
            theme_text_color : "Primary"
            text_color: 0,0,1,1
        MDRoundFlatButton : 
            text :'Calculadora'
            on_release: app.root.current = 'calculadora'
            pos_hint : {"center_x": 0.5, "center_y": 0.5}
            theme_text_color : "Primary"
            text_color: 0,0,1,1
        MDRoundFlatButton :
            text: 'Exercícios'
            on_release: app.root.current = 'exercicios'
            pos_hint : {"center_x": 0.5, "center_y": 0.5}
            text_color: 0,0,1,1

<Introducao1>:
    BoxLayout:
        orientation: "vertical"                
        MDToolbar:
            title :"Introdução a equação do segundo grau"
            left_action_items : [["arrow-left", lambda x : root.voltar()]]
            MDIconButton:
                icon: "numeric-2-circle-outline"
                on_release : app.root.current = 'introducao2'
            MDIconButton:
                icon: "numeric-3-circle-outline"
                on_release : app.root.current = 'introducao3'
            MDIconButton:
                icon: "numeric-4-circle-outline"
                on_release : app.root.current = 'introducao4'
            MDIconButton:
                icon: "calculator"
                on_release : app.root.current = 'calculadora'
            MDIconButton:
                icon: "pencil-outline"
                on_release : app.root.current = 'exercicios'   

        BoxLayout:
            orientation:'vertical'
            padding : 10,10,10,10
            spacing: dp(10)
            MDLabel:
                text_size: self.width - 10, None
                height : self.texture_size[1]
                text: root.string_intro

